# mkinitcpio preset file for my custom kernel :)
ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-jvyden"
ALL_microcode=(/boot/*-ucode.img)

PRESETS=('default' 'fallback')

default_image="/boot/initramfs-linux-jvyden.img"

fallback_image="/boot/initramfs-linux-jvyden-fallback.img"
fallback_options="-S autodetect"
